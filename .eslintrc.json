{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-console": "warn",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always"
      }
    ]
  }
}
